{
  "name": "shoplineart-complete",
  "version": "1.0.0",
  "description": "PEXELS gallery browser and photos downloader",
  "main": "index.js",
  "scripts": {
    "start": "node server/index.js",
    "server": "nodemon server/index.js",
    "client": "npm run dev --prefix client",
    "dev": "concurrently \"npm start\" \"npm run client\"",
    "render-postbuild": "NPM_CONFIG_PRODUCTION=false npm install --prefix client && npm run build --prefix client"
  },
  "keywords": [
    "pexels",
    "gallery",
    "photos",
    "download",
    "catevika",
    "vite",
    "react",
    "emailjs",
    "jszip",
    "responsive",
    "learning"
  ],
  "author": "Catevika",
  "license": "MIT",
  "dependencies": {
    "concurrently": "^7.6.0"
  }
}